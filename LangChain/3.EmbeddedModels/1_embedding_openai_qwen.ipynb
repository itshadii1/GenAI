    "from dotenv import load_dotenv\n",
    "\n",
    "load_dotenv()\n",
    "\n",
    "embeding = OpenAIEmbeddings(model='text-embeddings-3-large', dimensons=32)\n",
    "\n",
    "result = embeding.embed_query(\"Delhi is the capital of India\")\n",
    "\n",
    "print(str(result))"
